_realname=graphviz
pkgbase="mingw-w64-${_realname}"
pkgname="${MINGW_PACKAGE_PREFIX}-${_realname}"
pkgver=2.38.0
pkgrel=1
pkgdesc="Graph Visualization Tools"
arch=('any')
url="http://www.graphviz.org"
license=('GPL')
makedepends=()
depends=()

source=(
  "http://www.graphviz.org/pub/graphviz/stable/SOURCES/graphviz-${pkgver}.tar.gz"
)
sha256sums=(
  "81aa238d9d4a010afa73a9d2a704fc3221c731e1e06577c2ab3496bdef67859e"
)

build() {
  cd graphviz-${pkgver}/
  ls -al
}

package() {
  pwd
}

