_realname=kbproto
pkgbase="mingw-w64-${_realname}"
pkgname=("${MINGW_PACKAGE_PREFIX}-${_realname}")
pkgver=1.0.7
pkgrel=1
pkgdesc="X11 XKB extension wire protocol (mingw-w64)"
arch=(any)
license=('custom')
url='http://xorg.freedesktop.org/'
source=("http://xorg.freedesktop.org/releases/individual/proto/${_realname}-${pkgver}.tar.bz2")
sha256sums=('f882210b76376e3fa006b11dbd890e56ec0942bc56e65d1249ff4af86f90b857')

build() {
  cd "${srcdir}/${_realname}-${pkgver}"
  ./configure \
    --prefix=${MINGW_PREFIX}
  make
}

package() {
  cd "${srcdir}/${_realname}-${pkgver}"
  make DESTDIR="${pkgdir}" install
}

